<ul>
  <% users.each do |user| %>
    <% if current_user.followings.include?(user) %>
      <li><%= user.username %><%= link_to '  -Unfollow', unfollow_user_path(user), method: :post %></li>
    <% else %>
      <li><%= user.username %><%= link_to '  +Follow', follow_user_path(user), method: :post %></li>
    <% end %>
  <% end %>
</ul>